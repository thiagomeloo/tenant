{
    "name": "thiagomeloo/tenant",
    "description": "A package to manage multi-tenant applications in Laravel",
    "keywords": [
        "php",
        "laravel",
        "multi-tenant",
        "tenant",
        "tenancy",
        "multi-tenancy"
    ],
    "type": "library",
    "license": "GNU GPLv3",
    "support": {
        "issues": "https://github.com/thiagomeloo/tenant/issues",
        "source": "https://github.com/thiagomeloo/tenant"
    },
    "authors": [
        {
            "name": "Thiago Melo",
            "email": "4thiagomelo5@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/events": "^10.0",
        "illuminate/queue": "^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.19",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Thiagomeloo\\Tenant\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thiagomeloo\\Tenant\\TenantServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}